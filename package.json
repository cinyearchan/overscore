{
  "name": "overscore",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "pnpm --filter \"./packages/*\" clean",
    "clean:module": "pnpm --filter \"./packages/*\" clean:module",
    "build": "pnpm --filter \"./packages/*\" build",
    "build:cjs": "pnpm --filter \"./packages/*\" build:cjs",
    "build:esm": "pnpm --filter \"./packages/*\" build:esm",
    "build:umd": "pnpm --filter \"./packages/*\" build:umd",
    "lint": "eslint packages/**/src/* packages/**/config/* test/*",
    "lint:fix": "pnpm run lint --fix",
    "lint:prettier": "prettier --check .",
    "test": "vitest run --reporter=verbose",
    "coverage": "vitest run --coverage",
    "ci": "commit",
    "cz": "git-cz",
    "prepare": "husky install",
    "doc": "jsdoc -c jsdoc.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@commitlint/format": "^12.1.1",
    "@commitlint/prompt-cli": "^16.3.0",
    "@overscore/utils": "workspace:^",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^5.2.0",
    "@pnpm/types": "^12.2.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "2.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "cdkit": "1.1.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "es5-shim": "^4.6.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jsdoc": "^4.0.4",
    "lint-staged": "^14.0.1",
    "minami": "^1.2.3",
    "prettier": "3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "source-map-support": "0.5.9",
    "taffydb": "^2.7.3",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "pnpm@8.10.5",
  "dependencies": {
    "vitest-environment-overscore": "link:browser-env/vitest-environment-overscore"
  }
}
