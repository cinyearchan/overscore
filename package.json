{
  "name": "overscore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "pnpm --filter \"./packages/*\" clean",
    "build": "pnpm --filter \"./packages/*\" build",
    "build:cjs": "pnpm --filter \"./packages/*\" build:cjs",
    "build:esm": "pnpm --filter \"./packages/*\" build:esm",
    "build:umd": "pnpm --filter \"./packages/*\" build:umd",
    "lint": "eslint packages/**/src/* packages/**/config/* test/*",
    "lint:fix": "pnpm run lint --fix",
    "lint:prettier": "prettier --check .",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "ci": "commit",
    "cz": "git-cz",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/register": "^7.22.15",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@commitlint/format": "^12.1.1",
    "@commitlint/prompt-cli": "^16.3.0",
    "@js-lib/cli": "^2.3.2",
    "@overscore/equal": "workspace:^",
    "@overscore/functional": "workspace:^",
    "@overscore/type": "workspace:^",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.13",
    "babel-jest": "^29.7.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "cdkit": "1.1.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "es5-shim": "^4.6.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "sinon": "^19.0.2",
    "source-map-support": "0.5.9"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "pnpm@8.10.5"
}
